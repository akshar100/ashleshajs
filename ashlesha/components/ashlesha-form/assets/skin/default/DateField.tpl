<script type="text/x-template" id="DateField-main">
	<div class="control-group">
    	<label for="input01" class="control-label">{LABEL}</label>
	    <div class="controls">
	     
	      <select class="dd" name="{FIELD_NAME}-dd">
	      	<option>--DD--</option>
	      </select>
	      <select class="mm" name="{FIELD_NAME}-mm">
	      	<option>--MM--</option>
	      </select>
	      <select class="yy" name="{FIELD_NAME}-yy">
	      	<option>--YY--</option>
	      </select>
	      <p class="help-block"></p>
	    </div>
    </div>
</script>